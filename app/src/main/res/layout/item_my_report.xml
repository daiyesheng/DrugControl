<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/layout_widthfilled_style"
    android:background="@drawable/item_stroke_selector"
    android:layout_marginLeft="@dimen/dp_10"
    android:layout_marginRight="@dimen/dp_10"
    android:layout_marginBottom="@dimen/dp_4"
    android:layout_marginTop="@dimen/dp_4"
    android:padding="@dimen/dp_10"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/tvProject"
        style="@style/item_tv_second_style"
        tools:text="心里测试项目" />


    <TextView
        android:id="@+id/tvResult"
        style="@style/item_tv_normal_style"
        tools:text="测试结果：78分 轻度吸毒者"
        android:layout_below="@+id/tvProject"
        android:layout_marginTop="@dimen/dp_10"/>

    <TextView
        android:id="@+id/tvDate"
        style="@style/item_tv_normal_style"
        tools:text="2019.08.15 15:30"
        android:layout_below="@+id/tvResult"
        android:layout_marginTop="@dimen/dp_12"/>

    <View
        android:id="@+id/line1"
        style="@style/line_style"
        android:layout_below="@+id/tvDate"
        android:layout_marginTop="@dimen/dp_10" />

    <Button
        android:layout_below="@+id/line1"
        android:layout_width="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginBottom="@dimen/dp_10"
        tools:text="查看详情"
        android:clickable="false"
        android:text="@string/checked_detail"
        style="@style/btn_style" />

</RelativeLayout>